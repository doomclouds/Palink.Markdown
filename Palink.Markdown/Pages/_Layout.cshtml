@using Microsoft.AspNetCore.Components.Web
@namespace Palink.Markdown.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

<!DOCTYPE html>
<html lang="en">
<head lang="zh-cn">
    <meta charset="utf-8"/>
    <meta content="width=device-width, initial-scale=1.0" name="viewport"/>
    <base href="~/"/>
    @* <link href="_content/PalinkBlazor.Markdown/editor.md/css/editormd.css" rel="stylesheet"/> *@
    @* <link href="_content/PalinkBlazor.Markdown/editor.md/css/editormd.preview.css" rel="stylesheet"/> *@
    @* <link href="Palink.Markdown.styles.css" rel="stylesheet"/> *@
    <component render-mode="ServerPrerendered" type="typeof(HeadOutlet)"/>
</head>
<body>
@RenderBody()
<div id="blazor-error-ui">
    <environment include="Staging,Production">
        An error has occurred. This application may no longer respond until reloaded.
    </environment>
    <environment include="Development">
        An unhandled exception has occurred. See browser dev tools for details.
    </environment>
    <a class="reload" href="">Reload</a>
    <a class="dismiss">🗙</a>
</div>
<script src="_framework/blazor.server.js"></script>
</body>
</html>